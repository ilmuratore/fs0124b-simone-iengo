{
  "extends": [
    "development"
  ],
  "hints": {
    "compat-api/css": [
      "default",
      {
        "ignore": [
          "text-underline-offset",
          "-webkit-text-size-adjust",
          "text-align: -webkit-match-parent",
          "print-color-adjust",
          "user-select",
          "mask-image",
          "mask-size",
          "mask-position",
          "-webkit-tap-highlight-color",
          "text-decoration-skip-ink",
          "user-select: all",
          "overflow-anchor",
          "-webkit-overflow-scrolling"
        ]
      }
    ],
    "axe/structure": [
      "default",
      {
        "list": "off"
      }
    ],
    "axe/aria": [
      "default",
      {
        "aria-progressbar-name": "off"
      }
    ],
    "no-inline-styles": "off",
    "axe/name-role-value": [
      "default",
      {
        "link-name": "off",
        "button-name": "off"
      }
    ],
    "disown-opener": "off",
    "axe/keyboard": [
      "default",
      {
        "nested-interactive": "off"
      }
    ],
    "axe/text-alternatives": [
      "default",
      {
        "image-alt": "off"
      }
    ]
  }
}