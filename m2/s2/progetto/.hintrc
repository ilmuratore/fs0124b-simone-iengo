{
  "extends": [
    "development"
  ],
  "hints": {
    "compat-api/css": [
      "default",
      {
        "ignore": [
          "-webkit-text-size-adjust",
          "text-align: -webkit-match-parent",
          "print-color-adjust",
          "user-select",
          "mask-image",
          "mask-size",
          "mask-position",
          "text-decoration-skip-ink",
          "-webkit-tap-highlight-color",
          "-webkit-overflow-scrolling",
          "overflow-anchor",
          "text-underline-offset",
          "user-select: all",
          "scale"
        ]
      }
    ],
    "axe/forms": [
      "default",
      {
        "label": "off"
      }
    ],
    "axe/name-role-value": [
      "default",
      {
        "link-name": "off"
      }
    ],
    "axe/text-alternatives": [
      "default",
      {
        "document-title": "off"
      }
    ]
  }
}